<div id="dvautosearchhotel" style="z-index:1001;position:fixed;left:40%;top:40%;width:35%;height:auto;display:none;background-color:#b6ff00;border:1px solid #b6ff00;"><div id="labelautosearch" style="display:block;float:left;position:relative;">Gõ tên khách sạn cần tìm..</div><input name="hotelnameauto" type="text" id="hotelnameauto" value="" class="form-control" style="display:block;float:left;position:relative;" onkeyup="searchHotelAuto();"/></div>      
        <script>
            document.addEventListener("keydown", autosearch);
            function autosearch(event) {
                var keyCode = ('which' in event) ? event.which : event.keyCode;
                //alert("The Unicode key code is: " + keyCode);
                if (keyCode == 18) {
                    $("#dvautosearchhotel").show();
                    document.getElementById("hotelnameauto").focus();
                }
                if (keyCode == 27) {
                    $("#dvautosearchhotel").hide();
                }
            }
